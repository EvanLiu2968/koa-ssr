/*
 * 公共头部栏
 */

.wrapper
  position:relative
  color:#333
  background:#fff
.main
  margin-top:50px
  padding:20px
.container
  margin-left:auto
  margin-right:auto
  width:980px

@media (max-width: 767px)
  .container
    width:100%

.header
  position:fixed
  top:0
  padding:0 20px
  width:100%
  height: 50px
  background:#fff
  box-shadow: 0 1px 3px rgba(26,26,26,.1);
  z-index:999
  .logo
    margin-top:10px
    width:auto
    height:30px
  .tag
    margin-left:10px
    padding:2px 4px
    font-size:12px
    color:#fff
    border-radius:3px
    background:#20a0ff
  .header-left
    position:relative
    float:left
  .header-right
    padding:10px 0
    float:right